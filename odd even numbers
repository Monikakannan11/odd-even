
public class Main
{
	public static void main(String[] args) {
	    int a[]={1,2,3,4,7,8,9};
	    int i,j;
	    for(i=0;i<a.length;i++)
	    if(a[i]%2==0){
		System.out.println("Even Number is"+a[i]);
	    }
	    for(j=0;j<a.length;j++)
	    if(a[j]%2!=0){
		System.out.println("Odd number is"+a[j]);
	    }
	}
}
